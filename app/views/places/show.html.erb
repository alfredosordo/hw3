
<h1> <%= @place["name"] %> </h1>


<ul>
<% for post in @posts %>
<h2><%= post["title"] %></h2>
<p><%= post["posted_on"] %></p>
<p><%= post["description"] %></p>
<% end %>
</ul>

<p><a href="/posts/new?place_id=<%= @place["id"] %>">New <%= @place["name"] %> Post</a><p>

<p><a href="/places">Back to All Places I've Been</a></p>